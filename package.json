{
  "name": "ionia-ui",
  "version": "0.0.0",
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">= 20.0.0",
    "pnpm": ">= 9.0.0"
  },
  "sideEffects": false,
  "files": [
    "./dist",
    "./preset.js"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./theme.css": "./dist/theme.css",
    "./package.json": "./package.json",
    "./preset": "./preset.js",
    "./atoms": {
      "import": "./dist/atoms/index.mjs",
      "require": "./dist/atoms/index.js",
      "types": "./dist/atoms/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "dev": "storybook dev -p 6006",
    "build": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=16384' tsup --dts",
    "lint": "eslint src --ext js,ts,jsx,tsx --fix",
    "release": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-table": "^8.9.3",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "embla-carousel-autoplay": "^8.3.1",
    "embla-carousel-react": "^8.3.1",
    "framer-motion": "^10.16.4",
    "lodash-es": "^4.17.21",
    "react-popper": "^2.3.0",
    "react-use": "^17.6.0",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@semantic-release/commit-analyzer": "^10.0.4",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.5",
    "@semantic-release/release-notes-generator": "^11.0.7",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.0",
    "@storybook/preview-api": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@swc/core": "^1.3.82",
    "@swc/plugin-transform-imports": "^1.5.75",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "chromatic": "^7.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-config-standard-with-typescript": "^36.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.41",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-icons": "^4.10.1",
    "semantic-release": "^21.1.1",
    "storybook": "^7.4.0",
    "tailwindcss": "^3.4.6",
    "tsup": "^8.2.4",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "tailwindcss": "^3.3.2"
  }
}
